import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns

# Sample DataFrame creation (use your own dataset here)
data = {
    'CREDIT_SCORE': np.random.randint(300, 850, 100),
    'ANNUAL_MILEAGE': np.random.randint(5000, 30000, 100),
    'SPEEDING_VIOLATIONS': np.random.randint(0, 10, 100),
    'PAST_ACCIDENTS': np.random.randint(0, 5, 100),
    'OUTCOME': np.random.randint(0, 2, 100)
}
df = pd.DataFrame(data)

# Calculating the correlation matrix
corr_matrix = df.corr()

# Plotting the heatmap
plt.figure(figsize=(10, 8))
sns.heatmap(corr_matrix, annot=True, cmap='Blues', linewidths=0.5, vmin=-1, vmax=1)
plt.title('Correlation Heatmap of Numerical Variables')
plt.show()





import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns

# Load your dataset (replace 'your_dataset.xlsx' with your actual file)
df = pd.read_excel('car_dataset.xlsx')

# Check column names and first few rows
print("Column names:")
print(df.columns)
print("\nFirst few rows of the dataset:")
print(df.head())

# Correct column names to match dataset if needed
df.columns = df.columns.str.strip()  # Strip any whitespace from column names

# Rename columns explicitly if necessary
# Assuming your column names in the Excel file are 'ANNUAL MILEAGE', 'CREDIT SCORE', 'VEHICLE TYPE'
# If different, adjust accordingly
df.rename(columns={'ANNUAL MILEAGE': 'Annual Mileage', 'CREDIT SCORE': 'Credit Score', 'VEHICLE TYPE': 'Vehicle Type'}, inplace=True)

# Check for missing values and handle them
df = df.dropna(subset=['Annual Mileage', 'Credit Score', 'Vehicle Type'])

# Example scatter plot: Annual Mileage vs Credit Score
plt.figure(figsize=(10, 6))

# Scatter plot with different colors for different vehicle types
sns.scatterplot(
    data=df,
    x='Annual Mileage',
    y='Credit Score',
    hue='Vehicle Type',  # Differentiate by Vehicle Type
    palette='viridis',
    alpha=0.7  # Transparency to help with overlapping points
)

# Customize the plot
plt.title('Annual Mileage vs Credit Score by Vehicle Type')
plt.xlabel('Annual Mileage')
plt.ylabel('Credit Score')
plt.legend(title='Vehicle Type')
plt.grid(True)

# Show the plot
plt.show()

import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns

# Load your dataset (replace 'your_dataset.xlsx' with your actual file)
df = pd.read_excel('car_dataset.xlsx')

# Check column names and first few rows
print("Column names:")
print(df.columns)
print("\nFirst few rows of the dataset:")
print(df.head())

# Correct column names to match dataset if needed
# Convert all column names to lowercase and replace spaces with underscores
df.columns = df.columns.str.lower().str.replace(' ', '_')

# Check updated column names
print("\nUpdated column names:")
print(df.columns)

# Now, use the updated column names in dropna
# Check for missing values and handle them
# Assuming your column names in the Excel file are 'ANNUAL MILEAGE', 'CREDIT SCORE', 'VEHICLE TYPE'
# Adjust the column names below according to your dataset
df = df.dropna(subset=['annual_mileage', 'credit_score', 'vehicle_type'])

# Example scatter plot: Annual Mileage vs Credit Score
plt.figure(figsize=(10, 6))

# Scatter plot with different colors for different vehicle types
sns.scatterplot(
    data=df,
    x='annual_mileage',  # Use updated column name
    y='credit_score',  # Use updated column name
    hue='vehicle_type',  # Use updated column name
    palette='viridis',
    alpha=0.7  # Transparency to help with overlapping points
)

# Customize the plot
plt.title('Annual Mileage vs Credit Score by Vehicle Type')
plt.xlabel('Annual Mileage')
plt.ylabel('Credit Score')
plt.legend(title='Vehicle Type')
plt.grid(True)

# Show the plot
plt.show()